{
  "expo": {
    "name": "taskify_frontend",
    "slug": "taskify_frontend",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./images/Taskifylogo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./images/splashScreen.png",
      "resizeMode": "contain",
      "backgroundColor": "#E90064"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./images/Taskifylogo.png",
        "backgroundColor": "#E90064"
      }
    },
    "web": {
      "favicon": "./images/Taskifylogo.png"
    }
  }
}
